{
  "configuration": {
      "status": "error",
      "name": "Harbinger",
      "packages": "dev.zontreck",
      "ThresholdFilter": {
          "level": "debug"
      },
      "appenders": {
          "Console": {
              "name": "STDOUT",
              "PatternLayout": {
                  "pattern": "%d [%t] %-5p %c - %m%n"
              }
          }
      },
      "loggers": {
          "root": {
              "level": "info",
              "AppenderRef": {
                  "ref": "STDOUT"
              }
          }
      }
  }
}